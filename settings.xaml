<Window x:Class="Yo_Tuk_Tuk_Epos.settings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Yo_Tuk_Tuk_Epos"
        mc:Ignorable="d"
        Title="settings" Height="450" Width="800">
    <Grid>
        <Viewbox>
            <Grid>
                <StackPanel Orientation="Horizontal">
                    <Button x:Name="dayEnd" Content="Day End" Background="#FF4EE715"  Style="{StaticResource tables}" Click="DayEnd_Click"/>
                    <Button x:Name="twbtn" Content="Settings"  Background="#FF4EE715" Style="{StaticResource tables}" FontSize="36" />
                </StackPanel>
            </Grid>
        </Viewbox>
        
    </Grid>
</Window>
